# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Environment settings
ENVIRONMENT=development
DEBUG=false

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_openai_org_id_here

# Google Services Configuration
GSPREED_KEY=your_google_sheets_key_here
GOOGLE_CREDENTIALS_PATH=/path/to/your/google-credentials.json

# Telegram Configuration
TELEGRAM_WEBHOOK_URL=https://your-telegram-webhook.com
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Instagram Configuration
INSTAGRAM_USERNAME=your_instagram_username

# File System Paths
AASHVI_BASE_PATH=/Users/your_username/path/to/aashvi-model

# Stable Diffusion Configuration
AUTOMATIC1111_URL_FILE=automatic1111_url.txt

# Timing Configuration (in minutes)
AUTOMATION_INTERVAL_MINUTES=30
STORY_INTERVAL_MINUTES=180
POSTING_INTERVAL_MINUTES=720
APPROVAL_CHECK_INTERVAL_MINUTES=30